<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.caiyf.appliance.repair.bs.dao.NationMapper">
    <select id="getAllPo" resultType="NationPo">
        SELECT `id`, `nation` FROM `nation`
        WHERE `is_deleted`=0;
    </select>

    <select id="getNationById" resultType="java.lang.String">
        SELECT `nation` FROM `nation`
        WHERE `id`=#{id} AND `is_deleted`=0;
    </select>
</mapper>